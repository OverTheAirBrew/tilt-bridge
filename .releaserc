{
  "branches": [
    "master",
    {
      "name": "next",
      "prerelease": true
    }
  ],
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/npm",
    "@semantic-release/github",
    [
      "@semantic-release/git",
      {
        "assets": [
          "dist/src/**/*.{js,css}",
          "package.json"
        ]
      }
    ],
    "@semantic-release/git",
    [
      "./docker-buildx",
      {
        "registryUrl": "docker.io",
        "name": "overtheairbrew/tilt-bridge"
      }
    ]
  ]
}
